@fontSize:15px;//默认字体
@fontColor:#333;//默认字体颜色：
@btn_color:#24c789;//按钮颜色：
@btn_hoverColor:#24c789;//按钮移入颜色：
@btn_hoverFontColor:#fff;//按钮移入字体颜色：
@btn_border:1px solid #24c789;//按钮边框：
@btn_radius:5px;//按钮的幅度：
@navHoverColor:#1e97a2;//导航栏移入颜色：
@bgColor:#FAFAFA;//背景颜色：
*{
margin: 0;
  padding: 0;
  list-style: none;

}
body{
  background-color:@bgColor ;
}
a {
  text-decoration:none;
  color: #333;
}
section{
  width: 1349px;

}
header{
  width: 1349px;
  height: 74px;
  h1{
    font-size: 30px;
    width: 160px;
    height: 74px;
    float: left;
    margin-left: 30px;
  }
  a{
    text-decoration: none;
    color: @fontColor;
    font-size: @fontSize;

  }
  .sel-page{
    width: 600px;
    float: left;
    margin-left: 60px;
    height: 74px;
    li{
      text-align: center;
      float: left;
      width: 200px;
      line-height: 74px;
      a{
        &:hover{
          color: @navHoverColor;
          font-weight: 600;
        }
      }
    }
  }
  .login-jump{
    width: 480px;
    height: 74px;
    float: right;
    li{
      text-align: center;
      float: left;
      width: 140px;
      line-height: 74px;
      img{
        width: 25px;
        height: 25px;
        // position: relative;
        // top:5px;
        // left: 0;
      }
      .person-center-img{
        display: inline-block;
        width: 25px;
        height: 25px;
        background: url('../images/shoppingmall/个人中心.png');
        background-size: 100% 100%;
        position: relative;
        top:5px;
        left: 0;
        border-radius: 50%
      }
      a{
        &:hover{
          color: @navHoverColor;
          font-weight: 600;
        }
      }
    }
  }
}
#c1-box1{
  overflow: hidden;
  margin: 13px auto;
width:770px;
  background-color: white;
text-align: center;
  .p1{
  font-size: 24px;
    color:@btn_color;
    font-weight: bold;
    margin-top:85px ;
  }
  p{
    color:#868686;
    font-size: 16px;
    margin-top: 20px;
  }
  ul{
margin-top:60px ;
    width: 770px;
    overflow: hidden;
    margin-bottom: 10px;

    li{
      width: 192.3px;
      height: 192.3px;
      float: left;
      overflow: hidden;
    //div:hover{
    //  background-color: rgba(0, 0, 0, 0.1);
    //  }
      div{
        text-align: center;
        width: 192.3px;
        height: 192.3px;
        position: absolute;
        background-color: rgba(0, 0, 0, 0.35);

      .p2{
        font-size: 19px;
        color: white;
        margin-top: 67px;
        font-weight: bold;
      }
        p{
          margin-top: 15px;
          color: #9b9b9b;
          font-size: 15px;

        }
      }
      img {
        width: 192.3px;
        height: 192.3px;
      }
    }
  }
button{
  outline: none;
  margin-top: 15px;
  background-color: white;
  border:@btn_border ;
  color: @btn_color;
border-radius:20px;
  width: 140px;
  height: 35px;
  line-height: 35px;
  margin-bottom: 35px;
}
  #p1-1{
    margin-top: 50px;
    font-size: 12px;
  }
  button:hover{
    color: white;
    background-color:@btn_color;
  }
}
