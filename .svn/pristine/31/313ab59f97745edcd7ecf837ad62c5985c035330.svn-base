                            $('#c1-box1 ul').append( '<li index="'+res.data[i].Type1_id+'"><a href="../pages/classtype2.html?uid='+uid+'&cid='+res.data[i].Type1_id+'"><div><p class="p2">'+res.data[i].Type1_name+'</p><p>'+res.data[i].num+'个课程</p></div><img src="'+res.data[i].Type1_src+'" alt=""></a></li>')
