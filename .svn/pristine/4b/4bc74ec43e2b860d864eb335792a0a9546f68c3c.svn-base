@fontSize:15px;//默认字体
@fontColor:#333;//默认字体颜色：
@btn_color:#24c789;//按钮颜色：
@btn_hoverColor:#24c789;//按钮移入颜色：
@btn_hoverFontColor:#fff;//按钮移入字体颜色：
@btn_border:1px solid #24c789;//按钮边框：
@btn_radius:5px;//按钮的幅度：
@navHoverColor:#1e97a2;//导航栏移入颜色：
@bgColor:#FAFAFA;//背景颜色：
*{
  padding: 0;
  margin: 0;
  list-style: none;
  color:@fontColor;
  text-decoration: none;
}
body{
  background:@bgColor;
}
label{
  display: inline-block;
  font-size: 16px;
  min-width: 120px;
  text-align: right;
  margin-right: 16px;
}
input{
  display: inline-block;
  padding: 0 10px;
  height: 30px;
  font-size: 16px;
  line-height: 1;
  color: #777;
  background: #FCFCFC;
  border: 1px solid #CCC;
  border-radius: 2px;
  width: 250px;
}
.header{
  width: 570px;
  height: 76px;
  border-bottom: 1px solid #ddd;
  margin: 80px auto 30px;
  text-align: center;
  .logo{
    width: 160px;
    height: 50px;
    background: url('./../images/djw/49484165.png')no-repeat;
    display: inline-block;
    h1{
      margin-left: 55px;
    }
  }
}
h3{
  text-align: center;
}
.red{
  font-size: 13px;
  color:red;
}
.green{
  font-size: 13px;
  color:green;
}

.name{
  margin-top: 20px;
  margin-left: 390px;
  span{
    //visibility: hidden;
  }
}
.user{
  margin-top: 20px;
  margin-left: 390px;
  span{
    visibility: hidden;
  }
}
.pass{
  margin-top: 20px;
  margin-left: 390px;
  span{
    visibility: hidden;
  }
}
.againpass{
  margin-top: 20px;
  margin-left: 390px;
  span{
    visibility: hidden;
  }
}
.code{
  margin-top: 20px;
  margin-left: 390px;
  span{
    font-size: 13px;
  }
  #inp1{
    width: 50px;
  }
  #inp2{
    width: 60px;
    margin-left: 47px;
    outline: 0;
    vertical-align: middle;
    background: antiquewhite;
    color: green;
  }
  a{
    color: #4c7cc6;
    &:hover{
      color: #73a3ed;
      text-decoration: underline;
    }
  }
}
.sure{
  width: 100px;
  height: 30px;
  margin-top: 10px;
  margin-left: 531px;
  background: @btn_color;
  color: #fff;
  border: @btn_border;
  border-radius: 2px;
  &:hover{
    opacity: 0.9;
  }
}
#tip{
   width: 100%;
   height: 100%;
   background: rgba(51,51,51,0.6);
   position: fixed;
   top: 0px;
   left: 0px;
   display: none;
   .big{
     margin: -93px 0px 0px -251px;
     position: fixed;
     top: 50%;
     left: 50%;
     display: block;
     border: 1px solid #b0cb83;
     width: 500px;
     height: 180px;
     background: #fff;
     color:#666;
     h4{
       background: @navHoverColor;
       color: #fff;
       font-size: 14px;
       height: 35px;
       line-height: 35px;
       overflow: hidden;
       padding: 0 10px;
       border-bottom: 1px solid @navHoverColor;
       a{
         font-size: 20px;
         font-weight: bold;
         color: #ccff99;
         float: right;
       }
     }
     .mid{
       width: 100%;
       height: 90px;
       p{
         line-height: 100px;
         text-align: center;
       }
     }
     .bot{
       width: 100%;
       height: 55px;
       div{
         text-align: center;
         line-height: 55px;
         a{
           width: 106px;
           height: 34px;
           line-height: 34px;
           background: @navHoverColor;
           border: 0;
           color: #fff;
           display: inline-block;
           cursor: pointer;
         }
       }
     }
   }
 }
//导航
header{
  width: 1349px;
  height: 74px;
  h1{
    font-size: 30px;
    width: 160px;
    height: 74px;
    float: left;
    margin-left: 30px;
  }
  a{
    text-decoration: none;
    color: @fontColor;
    font-size: @fontSize;

  }
  .sel-page{
    width: 600px;
    float: left;
    margin-left: 60px;
    height: 74px;
    li{
      text-align: center;
      float: left;
      width: 200px;
      line-height: 74px;
      a{
        &:hover{
          color: @navHoverColor;
          font-weight: 600;
        }
      }
    }
  }
  .login-jump{
    width: 480px;
    height: 74px;
    float: right;
    li{
      text-align: center;
      float: left;
      width: 140px;
      line-height: 74px;
      img{
        width: 25px;
        height: 25px;
        // position: relative;
        // top:5px;
        // left: 0;
      }
      .person-center-img{
        display: inline-block;
        width: 25px;
        height: 25px;
        background: url('../images/shoppingmall/个人中心.png');
        background-size: 100% 100%;
        position: relative;
        top:5px;
        left: 0;
        border-radius: 50%
      }
      a{
        &:hover{
          color: @navHoverColor;
          font-weight: 600;
        }
      }
    }
  }
}