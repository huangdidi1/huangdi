* {
  padding: 0;
  margin: 0;
  list-style: none;
}
body {
  width: 1349px;
  background-color: #FAFAFA;
}
a {
  text-decoration: none;
  color: #333;
}
header {
  width: 1349px;
  height: 74px;
}
header h1 {
  font-size: 30px;
  width: 160px;
  height: 74px;
  float: left;
  margin-left: 30px;
}
header a {
  text-decoration: none;
  color: #333;
  font-size: 15px;
}
header .sel-page {
  width: 600px;
  float: left;
  margin-left: 60px;
  height: 74px;
}
header .sel-page li {
  text-align: center;
  float: left;
  width: 200px;
  line-height: 74px;
}
header .sel-page li a:hover {
  color: #1e97a2;
  font-weight: 600;
}
header .login-jump {
  width: 480px;
  height: 74px;
  float: right;
}
header .login-jump li {
  text-align: center;
  float: left;
  width: 140px;
  line-height: 74px;
}
header .login-jump li img {
  width: 25px;
  height: 25px;
}
header .login-jump li .person-center-img {
  display: inline-block;
  width: 25px;
  height: 25px;
  background: url('../images/shoppingmall/个人中心.png');
  background-size: 100% 100%;
  position: relative;
  top: 5px;
  left: 0;
  border-radius: 50%;
}
header .login-jump li a:hover {
  color: #1e97a2;
  font-weight: 600;
}
#a2-div1 {
  height: 53px;
  background-color: white ;
}
#a2-div1 span {
  font-size: 12px;
  color: #868686;
  margin-left: 310px ;
  height: 53px;
  line-height: 53px;
}
#a2-div1 #span1 {
  margin-left: 575px;
}
#a2-div2 {
  text-align: center;
  width: 768px;
  margin: 10px auto;
  background-color: white;
  overflow: hidden;
}
#a2-div2 ul {
  margin-top: 55px;
  width: 718px;
  margin-left: 43px;
  overflow: hidden;
}
#a2-div2 ul li {
  text-align: left;
  float: left;
  width: 331px;
  height: 87px ;
  margin: 0 0 35px 14px ;
}
#a2-div2 ul li p {
  font-size: 14px;
  color: #575757;
  margin-left: 146px;
  margin-top: 10px;
}
#a2-div2 ul li .c3-p3 {
  color: #8b8b8b;
  font-size: 12px;
  margin-top: 5px;
  font-weight: bold;
}
#a2-div2 ul li img {
  float: left;
  width: 136px;
  height: 87px;
}
