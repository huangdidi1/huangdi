* {
  padding: 0;
  margin: 0;
  list-style: none;
}
body {
  width: 1349px;
  background-color: #FAFAFA;
}
header {
  width: 1349px;
  height: 74px;
}
header h1 {
  font-size: 30px;
  width: 160px;
  height: 74px;
  float: left;
  margin-left: 30px;
}
header a {
  text-decoration: none;
  color: #333;
  font-size: 15px;
}
header .sel-page {
  width: 600px;
  float: left;
  margin-left: 60px;
  height: 74px;
}
header .sel-page li {
  text-align: center;
  float: left;
  width: 200px;
  line-height: 74px;
}
header .sel-page li a:hover {
  color: #1e97a2;
  font-weight: 600;
}
header .login-jump {
  width: 480px;
  height: 74px;
  float: right;
}
header .login-jump li {
  text-align: center;
  float: left;
  width: 140px;
  line-height: 74px;
}
header .login-jump li img {
  width: 25px;
  height: 25px;
}
header .login-jump li .person-center-img {
  display: inline-block;
  width: 25px;
  height: 25px;
  background: url('../images/shoppingmall/个人中心.png');
  background-size: 100% 100%;
  position: relative;
  top: 5px;
  left: 0;
  border-radius: 50%;
}
header .login-jump li a:hover {
  color: #1e97a2;
  font-weight: 600;
}
#c2-div1 {
  height: 53px;
  background-color: white ;
}
#c2-div1 a {
  text-decoration: none;
  color: #333;
}
#c2-div1 span {
  font-size: 12px;
  color: #868686;
  margin-left: 310px ;
  height: 53px;
  line-height: 53px;
}
#c2-div1 #span1 {
  margin-left: 575px;
}
#c2-ul {
  margin: 13px auto;
  width: 768px;
}
#c2-ul .c2-li {
  float: left;
  width: 368px;
  height: 288px;
  margin: 0 6px 12px 10px;
  position: relative;
  box-shadow: 1px 1px 1px 1px #eeeeee;
  overflow: hidden;
}
#c2-ul img {
  width: 368px;
  position: absolute;
}
#c2-ul .div2 {
  width: 368px;
  height: 88px;
  position: absolute;
  top: 126px;
  background-color: rgba(0, 0, 0, 0.35);
}
#c2-ul .div2 .div2-p1 {
  font-size: 18px;
  margin-top: 23px;
  font-weight: bold;
}
#c2-ul .div2 p {
  margin-left: 17px;
  margin-top: 8px;
  font-size: 12px;
  color: #fff;
}
#c2-ul .div2-2 {
  width: 368px;
  height: 74px;
  position: absolute;
  top: 214px;
  background-color: white;
}
#c2-ul .div2-2 p {
  font-size: 12px;
  color: #575757;
  margin-top: 17px;
  margin-left: 14px;
}
#c2-ul .div2-2 div {
  overflow: hidden;
  margin-top: 12px;
  margin-left: 14px ;
}
#c2-ul .div2-2 div p {
  float: left;
  font-size: 13px;
  color: #575757;
  margin-top: 0;
  margin-left: 0;
}
#c2-ul .div2-2 ul {
  float: left;
  margin-top: 2px;
  margin-left: 3px;
}
#c2-ul .div2-2 li {
  float: left;
  width: 13px;
  height: 13px;
}
#c2-ul .div2-2 li img {
  width: 13px;
  height: 14px;
  opacity: 0.4;
}
