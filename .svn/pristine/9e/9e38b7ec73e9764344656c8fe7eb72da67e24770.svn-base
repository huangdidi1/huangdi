* {
  padding: 0;
  margin: 0;
  list-style: none;
  color: #333;
  text-decoration: none;
}
.red {
  font-size: 13px;
  color: red;
}
.green {
  font-size: 13px;
  color: green;
}
.fnd {
  margin-top: 125px;
  margin-left: 200px;
}
.header .opone {
  color: #0a6cd6;
  opacity: 0.5;
}
.header .optwo {
  color: #4c7cc6;
}
.header {
  width: 730px;
  height: 45px;
  margin: 0 auto;
}
.header h1 {
  display: inline-block;
  font-weight: inherit;
  font-style: italic;
  opacity: 0.6;
}
.header p {
  display: inline-block;
  color: #666;
  font-size: 18px;
  margin: 0 0.2em;
  font-weight: bold;
}
label {
  display: inline-block;
  font-size: 16px;
  min-width: 120px;
  text-align: right;
  margin-right: 16px;
}
input {
  display: inline-block;
  padding: 0 10px;
  height: 30px;
  font-size: 16px;
  line-height: 1;
  color: #777;
  background: #FCFCFC;
  border: 1px solid #CCC;
  border-radius: 2px;
  width: 250px;
}
.partone .partone-top {
  margin-top: 20px;
  margin-left: 390px;
}
.partone .partone-top span {
  visibility: hidden;
}
.partone .partone-bot {
  margin-top: 20px;
  margin-left: 390px;
}
.partone .partone-bot .inp1 {
  width: 50px;
}
.partone .partone-bot span {
  font-size: 13px;
}
.partone .partone-bot .inp2 {
  outline: 0;
  vertical-align: middle;
  background: antiquewhite;
  color: green;
  margin-left: 56px;
  width: 60px;
}
.partone .partone-bot a {
  color: #4c7cc6;
}
.partone .partone-bot a:hover {
  color: #73a3ed;
  text-decoration: underline;
}
.partone .nextone {
  width: 50px;
  height: 30px;
  margin-top: 20px;
  margin-left: 532px;
  background: #169bd5;
  color: #fff;
  border: 1px solid #169bd5;
  border-radius: 2px;
}
.partone .nextone:hover {
  opacity: 0.9;
}
