}
.partfour {
  width: 450px;
  height: 66px;
  margin: 100px auto 0;
}
.partfour div {
  display: inline-block;
  width: 300px;
  height: 66px;
  vertical-align: top;
}
.partfour div p:first-child {
  color: black;
}
.partfour div p:last-child {
